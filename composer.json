{
    "name": "amaurycarrade/flarum-ext-syndication",
    "description": "Brings RSS and Atom feeds to Flarum",
    "license": "CECILL-B",
    "type": "flarum-extension",
    "keywords": [
        "flarum"
    ],
    "authors": [
        {
            "name": "Amaury Carrade",
            "email": "amaury@carrade.eu",
            "role": "Developer"
        }
    ],
    "require": {
        "flarum/core": "^0.1.0-beta.14"
    },
    "extra": {
        "flarum-extension": {
            "title": "Syndication",
            "icon": {
                "name": "fas fa-rss",
                "backgroundColor": "#e28f12",
                "backgroundRepeat": "no-repeat",
                "backgroundPosition": "center",
                "color": "#fff"
            }
        },
        "flagrow": {
            "discuss": "https://discuss.flarum.org/d/4395-syndication-extension-rss-atom-feeds"
        }
    },
    "autoload": {
        "psr-4": {
            "AmauryCarrade\\FlarumFeeds\\": "src/"
        }
    }
}
